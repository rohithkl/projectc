pipeline {
  agent any
    stages {
    
      stage('BUILD') {
        steps {
         echo "this is a Build stage" 
        }
      }
      
      stage('test') {
        steps {
         echo "this is a run test cases" 
        }
      }
      
       stage('deploy') {
        steps {
         echo "this is a Build stage" 
        }
      }
      
    }
}
